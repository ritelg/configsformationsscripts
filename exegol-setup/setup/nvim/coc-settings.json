{
  "explorer.width": 30,
  "coc.preferences.formatOnSaveFiletypes": ["markdown", "javascript", "html", "typescript"],
  "explorer.icon.enableNerdfont": true, 
  "explorer.file.showHiddenFiles": true,
  "suggest.noselect": false,
  "emmet.includeLanguages": {
    "scss": "css",
    "sass": "css",
    "markdown": "html",
    "html.twig": "html",
    "twig": "html",
    "*.twig": "html",
    "html.twig.js.css": "html",
    "*.ejs": "html",
    "ejs": "html"
  },
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "ejs"
  ],
  "snippets.ultisnips.pythonPrompt": false,
  "snippets.userSnippetsDirectory": "/root/.config/nvim/ultisnips",
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/16.0.2/clangd_16.0.2/bin/clangd"
}
